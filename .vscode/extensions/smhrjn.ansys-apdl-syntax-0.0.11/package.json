{
	"name": "ansys-apdl-syntax",
	"displayName": "ansys-apdl-syntax",
	"description": "syntax highlighting for ansys apdl",
	"version": "0.0.11",
	"publisher": "smhrjn",
	"engines": {
		"vscode": "^1.13.0"
	},
	"categories": [
		"Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "apdl",
				"aliases": [
					"apdl",
					"apdl"
				],
				"extensions": [
					".in",
					".out",
					".inp",
					".mac",
					".ans"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "apdl",
				"scopeName": "source.apdl",
				"path": "./syntaxes/apdl.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "apdl",
				"path": "./snippets/apdl.json"
			}
		]
	},
	"__metadata": {
		"id": "3064896e-dfac-4313-934e-1798758a9536",
		"publisherId": "b9464014-26ec-461f-8a75-408345decb17",
		"publisherDisplayName": "Sarat Maharjan"
	}
}