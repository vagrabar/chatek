{
  "comments": {
    "lineComment": "!"
  },

  "brackets": [["{", "}"], ["[", "]"], ["(", ")"], ["\\%", "\\%"]],

  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
    ["\\%", "\\%"]
  ],

  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
    ["\\%", "\\%"]
  ],

  "folding": {
    "markers": {
      "start": "^\\s*!*[\\s!]*#?region",
      "end": "^\\s*!*[\\s!]*#?endregion"
    }
  }
}
